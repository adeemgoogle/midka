 public class ATM
 {
     /// <summary>
     /// Name of the index in elasticsearch
     /// </summary>
     public static string IndexName = "atm";

     /// <summary>
     /// ATM unique number
     /// </summary>
     [JsonProperty("id")]
     [Column("atm_id")]
     public string Id { get; set; }

     /// <summary>
     /// Filial of bank from reference
     /// </summary>
     [JsonProperty("filial")]
     [Column("filial")]
     public string Filial { get; set; }

     /// <summary>
     /// Name of the locality (village / town) from "KATO" reference
     /// </summary>
     [JsonProperty("locality")]
     [Column("locality")]
     public string Locality { get; set; }

     /// <summary>
     /// Code of the locality (village / town) from "KATO" reference
     /// </summary>
     [JsonProperty("kato")]
     [Column("kato")]
     public string KatoCode { get; set; }

     /// <summary>
     /// Name of street/microdistrict, house number on rus
     /// </summary>
     [JsonProperty("adress")]
     [Column("adress")]
     public string Adress { get; set; }

     /// <summary>
     /// Name of street/microdistrict, house number on kaz
     /// </summary>
     [JsonProperty("adressKaz")]
     [Column("adress_kaz")]
     public string AdressKaz { get; set; }

     /// <summary>
     /// Branch number/government office name/trade center name
     /// </summary>
     [JsonProperty("location")]
     [Column("location")]
     public string Location { get; set; }

     /// <summary>
     /// Cash-in availability
     /// </summary>
     [JsonProperty("cash_in_fl")]
     [Column("cashin_flag")]
     public bool CashInFlag { get; set; }

     /// <summary>
     /// true - if public access available or limited access - false
     /// </summary>
     [JsonProperty("public_fl")]
     [Column("public_flag")]
     public bool PublicFlag { get; set; }

     /// <summary>
     /// true = if all-day available, otherwise - false
     /// </summary>
     [JsonProperty("allDay_fl")]
     [Column("allday_flag")]
     public bool AllDayFlag { get; set; }

     /// <summary>
     /// Weekdays working schedule
     /// </summary>
     [JsonProperty("weekday_time")]
     [Column("")]
     public WorkTime WeekdayTime { get; set; }

     /// <summary>
     /// Saturday working schedule
     /// </summary>
     [JsonProperty("sat_work_time")]
     [Column("sat_work_time")]
     public WorkTime SaturdayWorkTime { get; set; }

     /// <summary>
     /// Sunday working schedule
     /// </summary>
     [JsonProperty("sun_work_time")]
     [Column("sun_work_time")]
     public WorkTime SundayWorkTime { get; set; }

     /// <summary>
     /// Array of currency codes, which can be used to cash out
     /// </summary>
     [JsonProperty("currency")]
     [Column("currency")]
     public List<string> Currency { get; set; }

     /// <summary>
     /// Array of currency codes, which can be used to cash-in
     /// </summary>
     [JsonProperty("cash_in_currency")]
     [Column("cash_in_currency")]
     public List<string> CashInCurrency { get; set; }

     /// <summary>
     /// Cash in currency usd
     /// </summary>
     [JsonProperty("cash_in_currency_USD")]
     [Column("cash_in_currency_USD")]
     public bool CashInCurrencyUSD {get;set;}

     /// <summary>
     /// Cash in currency eur
     /// </summary>
     [JsonProperty("cash_in_currency_EUR")]
     [Column("cash_in_currency_EUR")]
     public bool CashInCurrencyEUR { get; set; }

     /// <summary>
     /// Cash in currency rub
     /// </summary>
     [JsonProperty("cash_in_currency_RUB")]
     [Column("cash_in_currency_RUB")]
     public bool CashInCurrencyRUB { get; set; }

     /// <summary>
     /// Cash out currency usd. Currency (cush-out)
     /// </summary>
     [JsonProperty("cash_out_currency_USD")]
     [Column("cash_out_currency_USD")]
     public bool CashOutCurrencyUSD { get; set; }

     /// <summary>
     /// Cash out currency usd. Currency (cush-out)
     /// </summary>
     [JsonProperty("cash_out_currency_EUR")]
     [Column("cash_out_currency_EUR")]
     public bool CashOutCurrencyEUR { get; set; }

     /// <summary>
     /// Cash out currency usd. Currency (cush-out)
     /// </summary>
     [JsonProperty("cash_out_currency_RUB")]
     [Column("cash_out_currency_RUB")]
     public bool CashOutCurrencyRUB { get; set; }

     /// <summary>
     /// Current work
     /// </summary>
     [JsonProperty("currentWork")]
     [Column("current_Work")]
     public bool CurrentWork { get; set; }

     /// <summary>
     /// Geolocation by utilising longitude and latitude
     /// </summary>
     [GeoPoint(Name = "getLocation")]
     [JsonProperty("geo_location")]
     public GeoLocation GetLocation { get; set; }

 }
