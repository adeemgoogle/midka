<Envelope>
    <Body>
        <sendMessage>
            <request>
                <messageInfo>
                    <messageId>your_message_id_here</messageId>
                    <correlationId>your_correlation_id_here</correlationId>
                    <sessionId>your_session_id_here</sessionId>
                </messageInfo>
            </request>
        </sendMessage>
    </Body>
</Envelope>


type Envelope struct {
	XMLName xml.Name
	Body    Body `xml:"Body"`
}

type Body struct {
	XMLName     xml.Name    `xml:"Body"`
	S           SendMessage `xml:"sendMessage"`
}

type SendMessage struct {
	XMLName xml.Name `xml:"sendMessage"`
	R       Request  `xml:"request"`
}
type Request struct {
	XMLName     xml.Name    `xml:"request"`
	M           MessageInfo `xml:"messageInfo"`
}

type MessageInfo struct {
	XMLName       xml.Name `xml:"messageInfo"`
	MessageId     string   `xml:"messageId"`
	CorrelationId string   `xml:"correlationId"`
	SessionId     string   `xml:"sessionId"`
}
