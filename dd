func (a *ABR) unmarshall(resp []byte) ([]*model.Transaction, error) {
	Resp := new(ABRResponse)
	
	data := new([]model.Transaction)
	
	
	err := json.Unmarshal(resp, &data)
	if err != nil {
		return nil, err
	}

	cashback := Resp.CashBack
	
	data[].Amount = cashback
	
	return data, nil
}
Invalid operation: 'data[]' (type '*[]model.Transaction' does not support indexing)
Cannot use 'data' (type *[]model.Transaction) as the type []*model.Transaction
