public override void Connect(StringBuilder startParam)
{
    if (IsConnected == false)
    {
        StringBuilder stringBuilder = new StringBuilder(256);

        int idx = MteConnect(startParam, stringBuilder);

        if (idx >= 0)
        {
            _isConnected = true;
            _procNum = idx;
            Console.WriteLine("Connected successfully");
        }
        else
        {
            _isConnected = false;
            SendErrorFromAsts(idx);
        }
    }
}


