FROM */aspnet:6.0 AS base 
WORKDIR /app
EXPOSE 80
EXPOSE 443

FROM */sdk:6.0 AS build
WORKDIR /src
COPY nuget.config .
COPY "src/TradePointsLoader/*.csproj" ./
RUN dotnet restore "./TradePointsLoader.csproj"
    
# Copy everything else and build
COPY loader/TradePointsLoader ./
WORKDIR "/src/."
RUN dotnet build "TradePointsLoader.csproj" -c release -o app/build

FROM build as publish
RUN dotnet publish "TradePointsLoader.csproj" -o /app/publish -c release --no-restore
    
# Build runtime image
FROM base
WORKDIR /app
COPY --from=publish /app/publish .
ENTRYPOINT ["dotnet", "TradePointsLoader.dll"]

