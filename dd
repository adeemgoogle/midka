func GetBonus(s *model.BonusResp) {
	s.Merchants[].MerchantId = sanitizeString(s.Merchants[].MerchantId)
	s.Merchants[].Error.ErrorDescription = sanitizeString(s.Merchants[].Error.ErrorDescription)
	s.Merchants[].Error.ErrorCode = sanitizeString(s.Merchants[].Error.ErrorCode)
	s.Merchants[].Phone = sanitizeString(s.Merchants[].Phone)
	balances := s.Merchants[].Balances
	balances[].Type = sanitizeString(balances[].Type)
}

func sanitizeString(s string) string {
	return html.UnescapeString(p.Sanitize(s))
}

