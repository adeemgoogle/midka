Field<ListGraphType<BranchType>>(
    "branch",
    arguments: new QueryArguments(
            new QueryArgument<StringGraphType> { Name = "id", Description = "Search Branch by Id" },
            new QueryArgument<CircleInputType> { Name = "circle", Description = "Search Branches in that circle" },
            new QueryArgument<StringGraphType> { Name = "address", Description = "address of the trade point" },
            new QueryArgument<StringGraphType> { Name = "kato", Description = "KATO code of locality" },
            new QueryArgument<IntGraphType> { Name = "pagingFrom", Description = "Pagination FROM value", DefaultValue = 0 },
            new QueryArgument<IntGraphType> { Name = "pagingSize", Description = "Number of paging size", DefaultValue = 4000 },
            new QueryArgument<BooleanGraphType> { Name = "corporateEntityFlag", Description = "Search Branches which can serve corporate entities or not", DefaultValue = null},
            new QueryArgument<BooleanGraphType> { Name = "embosserFlag", Description = "Search Branches which have embossers or not", DefaultValue = null}
    ),
    resolve: GetResolveContext);

