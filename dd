func GetExpTime(token string) (int64, error) {
	tokenStr, _, err := new(jwt.Parser).ParseUnverified(token, jwt.MapClaims{})
	if err != nil {
		return 0, err
	}
	if claims, ok := tokenStr.Claims.(jwt.MapClaims); ok {
		if exp, ok := claims["exp"].(float64); ok {
			return int64(exp), nil
		}
	}
	return 0, err
}

func CheckToken(tokenExp string) (bool, error) {
	if tokenExp != "" {
		tokenTme, err := GetExpTime(tokenExp)
		if err != nil {
			return false, errors.New("The token is wrong")
		}
		now := time.Now().Unix()
		tokenCreatedat := tokenTme - 5*60*60

		if now > tokenCreatedat {
			return true, nil
		}
	}
	return false, errors.New("wrong Tokens")
}
проверь, если токен живет больше чем 5 часов то он должен вернуть false
